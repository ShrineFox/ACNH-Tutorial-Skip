flow RecoveringNotice():
    EventFlowSystemActor.OpenMessageWindow('Dialog/DIALOG_Msg:9010', false)
    EventFlowSystemActor.SaveOnBackupRestoringSettingNotice()
 
flow StartCheck():
    if (not EventFlowSystemActor.SaveLastBackupElapsedDaysGreaterThan(7)) and (EventFlowSystemActor.SaveIsOnBackupRestoringSettingNotice()):
        EventFlowSystemActor.OpenMessageWindow('Dialog/DIALOG_Msg:9006', false)
        EventFlowSystemActor.SaveOnBackupRestoringSettingNotice()
 
