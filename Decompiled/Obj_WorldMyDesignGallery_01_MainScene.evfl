flow MainUI():
    EventFlowSystemActor.UINetConnectWaitDisappearEventFlowSystemActor.UINetConnectWaitDisappear()
    EventFlowSystemActor.UIMyDesignExchangeMenuHandlingEventFlowSystemActor.UIMyDesignExchangeMenuHandling()
    switch EventFlowSystemActor.MyDesignExchangeMenuResultEventFlowSystemActor.MyDesignExchangeMenuResult():
        case 0:
            run Seq_Close()
        case 1:
            EventFlowSystemActor.MyDesignExchangeResetErrorEventFlowSystemActor.MyDesignExchangeResetError()
            run Obj_WorldMyDesignGallery_04_Search::SearchByMyDesignIDrun Obj_WorldMyDesignGallery_04_Search::SearchByMyDesignID()
        case 2:
            run Obj_WorldMyDesignGallery_04_Search::SearchByAuthorIDrun Obj_WorldMyDesignGallery_04_Search::SearchByAuthorID()
        case 3:
            run Obj_WorldMyDesignGallery_03_Upload::Rootrun Obj_WorldMyDesignGallery_03_Upload::Root()
        case 4:
            run Obj_WorldMyDesignGallery_05_MyOpus::Rootrun Obj_WorldMyDesignGallery_05_MyOpus::Root()
        case 5:
            run Obj_WorldMyDesignGallery_06_Search2::SearchByConditionrun Obj_WorldMyDesignGallery_06_Search2::SearchByCondition()
        case 6:
            run Obj_WorldMyDesignGallery_06_Search2::SearchByFavoriteAuthorrun Obj_WorldMyDesignGallery_06_Search2::SearchByFavoriteAuthor()
 
flow Root():
    EventFlowSystemActor.SystemFaderReleaseEventFlowSystemActor.SystemFaderRelease()
    EventFlowSystemActor.WaitFrameEventFlowSystemActor.WaitFrame(30)
    if EventFlowSystemActor.IsInspectingWherearenEventFlowSystemActor.IsInspectingWherearen():
        run Obj_WorldMyDesignGallery_20_Wherearen::SearchByAuthorIDrun Obj_WorldMyDesignGallery_20_Wherearen::SearchByAuthorID()
    elif not EventFlowSystemActor.NetIsDreamingEventFlowSystemActor.NetIsDreaming():
        run MainUI()
    elif (EventFlowSystemActor.NetHasDesignerIDEventFlowSystemActor.NetHasDesignerID()) and (EventFlowSystemActor.NetDreamDesignerIsMeEventFlowSystemActor.NetDreamDesignerIsMe()):
        run Obj_WorldMyDesignGallery_10_InDream::SearchByMyOpusrun Obj_WorldMyDesignGallery_10_InDream::SearchByMyOpus()
    else:
        run Obj_WorldMyDesignGallery_10_InDream::SearchByAuthorIDrun Obj_WorldMyDesignGallery_10_InDream::SearchByAuthorID()
 
flow Seq_Close():
    EventFlowSystemActor.MessageSuspendEventFlowSystemActor.MessageSuspend()
    EventFlowSystemActor.WaitFrameEventFlowSystemActor.WaitFrame(15)
    if EventFlowSystemActor.IsInspectingWherearenEventFlowSystemActor.IsInspectingWherearen():
        EventFlowSystemActor.ReturnStageEventFlowSystemActor.ReturnStage('', 'cMyDesignBack', 'cMyDesignAppBack', 'cBlack', 0.5, 0.5)
    elif System.EventFlags['cPlayerTemp:EnterSceneByMydesignShowcaseApp']:
        EventFlowSystemActor.ReturnStageEventFlowSystemActor.ReturnStage('', 'cMyDesignBack', 'cMyDesignAppBack', 'cBlack', 0.5, 0.5)
    else:
        if EventFlowSystemActor.NetIsDreamingEventFlowSystemActor.NetIsDreaming():
            EventFlowSystemActor.CameraReserveFirstStateCommandEventFlowSystemActor.CameraReserveFirstStateCommand('Talk:MyDesignExchangeInDream', '', false)
        else:
            EventFlowSystemActor.CameraReserveFirstStateCommandEventFlowSystemActor.CameraReserveFirstStateCommand('Talk:MyDesignExchange', '', false)
        EventFlowSystemActor.ReturnStageEventFlowSystemActor.ReturnStage('', 'cMyDesignBack', 'cMyDesignBack', 'cBlack', 0.5, 0.5)
 
