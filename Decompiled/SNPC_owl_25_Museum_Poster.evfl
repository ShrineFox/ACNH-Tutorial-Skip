local flow Sub_grp_Event65():
	if not EventFlowSystemActor.HasBellCount(5000):
		# Eh wot? It appears you're short on Bells at the moment. I'm afraid I can't discount these, even for you.
		MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:013', false)
	elif EventFlowSystemActor.SystemHasBaggageSpace('cPocketBag') < 1:
		# Eh wot? But you don't have enough pocket space. You'll have to come back after you've made room.
		MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:014', false)
	else:
		if System.EventFlags['cPlayerTemp:OwlSellPosterCount'] >= 2:
			MainNpc.SetItemName(1, 0, 0)
			# That will be <90:26:0000> for your <125:0:0000>. Do I have that right?
			MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:012', false)
		if EventFlowSystemActor.GeneralTalkChoice2() == 0:
			# Thank you very much!
			MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:015', true)
			MainNpc.SetDeliveryItemAtRandom(2118, false, 'cVillageRemakePattern', 0)
			MainNpc.NpcDelivery(0, 'Default')
			EventFlowSystemActor.BellCountDown(5000, true)
			EventFlowSystemActor.WaitFrame(15)
			MainNpc.SetDeliveryItem(1, true)
			MainNpc.NpcDelivery(1, 'Default')
			System.EventFlags['cLifeSupportDaily:BuyItem'] += 5000
			System.EventFlags['cLifeSupportAchievement:HowmuchBuyItem'] += 5000
			if System.EventFlags['cPlayerTemp:OwlSellPosterCount'] < 2:
				EventFlowSystemActor.UIMoneyDisappear()
				run SNPC_owl_00_Common::Again()
			else:
				# Are there any other posters you'd like to purchase?
				MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:016', false)
				if EventFlowSystemActor.GeneralTalkChoice2() == 0:
					run Owl_BuyPoster()
				else:
					# Understood. Please let me know if you ever wish to purchase another poster. I would be only too pleased!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:017', false)
