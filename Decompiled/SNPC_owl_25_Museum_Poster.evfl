	if EventFlowSystemActor.MuseumKindDonateRate(100, 'cKindInsect'):
		if System.EventFlags['cLand:MuseumPosterflagbug']:
			if not System.EventFlags['cPlayer:MuseumPosterTodayflagbug']:
				EventFlowSystemActor.SelectRewardItemDirect(12039, 1, 'cVillageRemakePattern', 0)
				System.EventFlags['cPlayer:MuseumPosterTodayflagbug'] = true
				run Owl_New_GetPoster()
		elif not System.EventFlags['cLand:MuseumPosterTodayflagbug']:
			EventFlowSystemActor.SelectRewardItemDirect(12039, 1, 'cVillageRemakePattern', 0)
			if SubflowResults@3[2] in (0, 1):
