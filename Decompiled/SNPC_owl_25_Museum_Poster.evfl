flow Chk_PosterSelUnlock():
	run ChkNoticeUnlockPoster()
	if SubflowResults@7[3] in (1, 2, 3, 4, 5, 6):
		if System.EventFlags['cPlayer:MuseumPosterUnlockBuySel']:
			switch SubflowResults@7[3]:
				case 1:
					# Ah yes! I sent you another poster. This one is to commemorate the completion of our…bug collection. At least, I think I sent you a poster of the bug …
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:024', false)
					run Sub_Event153()
				case 2:
					# Ah yes! I sent you another poster, this time to celebrate the completion of our marvelous fish collection!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:024_01', false)
					run Sub_Event153()
				case 3:
					# Ah yes! I sent you another poster, this time to mark the completion of our beautiful sea creature collection!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:024_02', false)
					run Sub_Event153()
				case 4:
					# Ah yes! I sent you another poster, this time to celebrate the completion of our prestigious fossil collection!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:024_03', false)
					run Sub_Event153()
				case 5:
					# Ah yes! I sent you another poster, this time to celebrate the completion of our illustrious art collection!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:024_04', false)
					run Sub_Event153()
				case 6:
					# Ah yes! I sent you more posters to celebrate the completion of each of our collections!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:024_05', false)
					run Sub_Event153()
				default:
					return
		else:
			System.EventFlags['cPlayer:MuseumPosterUnlockBuySel'] = true
			if SubflowResults@7[3] in (0, 1, 2, 3, 4, 5):
				# Ah! That reminds me. I sent something to you in the mail. Did you happen to see it?
				MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:021', false)
			else:
				# Ah! That reminds me. I sent a few things to you in the mail. Did you happen to get them?
				MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:021_pl', false)
			switch SubflowResults@7[3]:
				case 1:
					# It was a poster commemorating the completion of our…bug collection. Or…I think it was. I couldn't bear to look at it!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:022', false)
					run Sub_Event146()
				case 2:
					# It was a poster commemorating the completion of our marvelous fish collection!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:022_01', false)
					run Sub_Event146()
				case 3:
					# It was a poster commemorating the completion of our beautiful sea creature collection!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:022_02', false)
					run Sub_Event146()
				case 4:
					# It was a poster commemorating the completion of our prestigious fossil collection!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:022_03', false)
					run Sub_Event146()
				case 5:
					# It was a poster commemorating the completion of our illustrious art collection!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:022_04', false)
					run Sub_Event146()
				case 6:
					# They were posters commemorating each of our completed collections!
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:022_05', false)
					run Sub_Event146()
				default:
					return

flow Owl_BuyPoster():
	EventFlowSystemActor.SetTagFromSystem('cFlow', 'cMoney', 0, 5000, '')
	if System.EventFlags['cPlayerTemp:OwlSellPosterCount'] < 2:
		MainNpc.SetItemName(1, 0, 0)
		# It will be <90:23:0000> to buy [a|an] <125:0:0000>. Would you like one?
		MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:018', false)
		if EventFlowSystemActor.GeneralTalkChoice2() == 0:
			run Sub_grp_Event65()
		else:
			run Sub_Event32()
	else:
		MainNpc.OwlPosterBuySelect()
		# Each poster costs <90:23:0000>. Which one would you like?
		MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:010', false)
		switch EventFlowSystemActor.PosterBuyChoice():
			case 0:
				EventFlowSystemActor.SelectRewardItemDirect(12039, 1, 'cVillageRemakePattern', 0)
				run Sub_grp_Event65()
			case 1:
				EventFlowSystemActor.SelectRewardItemDirect(12040, 1, 'cVillageRemakePattern', 0)
				run Sub_grp_Event65()
			case 2:
				EventFlowSystemActor.SelectRewardItemDirect(12985, 1, 'cVillageRemakePattern', 0)
				run Sub_grp_Event65()
			case 3:
				EventFlowSystemActor.SelectRewardItemDirect(12961, 1, 'cVillageRemakePattern', 0)
				run Sub_grp_Event65()
			case 4:
				EventFlowSystemActor.SelectRewardItemDirect(14589, 1, 'cVillageRemakePattern', 0)
				run Sub_grp_Event65()
			case 5:
				run Sub_Event32()

flow Owl_Common_PutInOrSend():
	switch SubflowResults@3[2]:
		case 0:
			MainNpc.SetEventRewardToPlayerBaggage('cFree')
			EventFlowSystemActor.SystemMailSendVillager('cOwl', 'MAIL_SNpc_owl', 2, 'cMuseumPoster', 'cFastToday', 'cReward')
		case 1:
			EventFlowSystemActor.SystemMailSend('cOwl', 'MAIL_SNpc_owl', 2, 'cMuseumPoster', 'cFastToday', 'cReward', false, false)
			EventFlowSystemActor.SystemMailSendVillager('cOwl', 'MAIL_SNpc_owl', 2, 'cMuseumPoster', 'cFastToday', 'cReward')
		case 2:
			EventFlowSystemActor.SystemMailSend('cOwl', 'MAIL_SNpc_owl', 2, 'cMuseumPoster', 'cFastTomorrow', 'cReward', false, false)
			EventFlowSystemActor.SystemMailSendVillager('cOwl', 'MAIL_SNpc_owl', 2, 'cMuseumPoster', 'cFastTomorrow', 'cReward')

flow Owl_GetPoster():
	if (EventFlowSystemActor.MuseumLevel() in (`BlathersMuseumArt`, `BlathersCafe`)) and (System.EventFlags['cPlayer:OwlMultiDonateExplain']) and (System.EventFlags['cPlayer:OwlShowDiveFishReport']):
		run Owl_Poster_CompJudge()
		if System.EventFlags['cPlayerTemp:OwlGetPosterCount'] != 0:
			if SubflowResults@2[4] == 0:
				# Oh yes! I would be remiss if I didn't mention the posters I have to mark each collection's completion. Every resident may claim one poster per comple…
				MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_11_Museum1stTalk:1100', false)
			else:
				# Oh yes! I just remembered something I wanted to tell you.
				MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:001', false)
				if (EventFlowSystemActor.MuseumComplete()) and (not System.EventFlags['cPlayer:FutaExplanationDonationComp']):
					# Thanks to the philanthropy of our contributors, we have filled ALL of our permanent exhibits. As the spirit of giving has made our halls complete, I …
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:001_01', false)
					System.EventFlags['cPlayer:FutaExplanationDonationComp'] = true
				if System.EventFlags['cPlayerTemp:OwlGetPosterCount'] < 2:
					# I decided to have a series of posters made to celebrate the completion of our collections. I felt it would be a good way to show my gratitude to ever…
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:002', false)
				else:
					# I decided to have a series of posters made to celebrate the completion of our collections. I felt it would be a good way to show my gratitude to ever…
					MainNpc.OpenMessageWindow('TalkSNpc/owl/SP_owl_25_Museum_Poster:002_pl', false)
			switch EventFlowSystemActor.FlagSystemIntValue6(6, 'cPlayerTemp:OwlGetPosterCount'):
				case 1:
					if EventFlowSystemActor.SystemHasBaggageSpace('cPocketBag') < 1:
	run Sub_Event24()
					else:
  run Sub_Event241()
				case 2:
					if EventFlowSystemActor.SystemHasBaggageSpace('cPocketBag') < 2:
	run Sub_Event24()
					else:
  run Sub_Event241()
				case 3:
					if EventFlowSystemActor.SystemHasBaggageSpace('cPocketBag') < 3:
	run Sub_Event24()
					else:
  run Sub_Event241()
				case 4:
					if EventFlowSystemActor.SystemHasBaggageSpace('cPocketBag') < 4:
	run Sub_Event24()
					else:
  run Sub_Event241()
				case 5:
					if EventFlowSystemActor.SystemHasBaggageSpace('cPocketBag') < 5:
	run Sub_Event24()
					else:
  run Sub_Event241()
				default:
					return

