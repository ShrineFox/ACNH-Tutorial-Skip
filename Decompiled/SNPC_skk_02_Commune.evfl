                # Very much obliged!
                MainNpc.OpenMessageWindowMainNpc.OpenMessageWindow('TalkSNpc/skk/SP_skk_02_Commune:013', true)
                run Demo_Common::Demo_Cmn_Shoppingrun Demo_Common::Demo_Cmn_Shopping()
