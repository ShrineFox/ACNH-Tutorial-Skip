flow CutPlaza():
	EventFlowSystemActor.CameraSetDemoParam('GrowupDemoStart', '', 'Place:EventPlaza', false, false, false, 0)
	EventFlowSystemActor.WaitFrame(5)
	EventFlowSystemActor.CameraSetDemoParam('GrowupDemoEnd', 'GrowupDemo', 'Place:EventPlaza', false, false, false, 0)
	EventFlowSystemActor.WaitFrame(150)

flow Root():
	run CutPlaza()
	EventFlowSystemActor.SystemRequestChangeStage('cGrowUpTalk', 'cCircle', 'cCircle', 'cBlack', 1.0, 1.0)
