flow EquipSet():
    if MainNpc.NpcLabelMainNpc.NpcLabel('man'):
        MainNpc.NpcSetEquipMainNpc.NpcSetEquip(14700, 0)
 
flow Init():
    MainNpc.NpcDisableServiceMotionMainNpc.NpcDisableServiceMotion()
    MainNpc.ServiceMotionControlMainNpc.ServiceMotionControl(false)
 
flow Root():
    run EquipSet()
    MainNpc.NpcChangeAIStateMainNpc.NpcChangeAIState('cWait', '', '', '', false, false)
 
