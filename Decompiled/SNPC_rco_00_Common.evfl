        SubflowResults[0] = 0
    elif (not EventFlowSystemActor.HasKindItem('cKind', 'Insect', '')) and (not EventFlowSystemActor.HasKindItem('cKind', 'Fish', '')):
        SubflowResults[0] = 0
    elif System.EventFlags['cPlayer:MakeVillagePlayerFlag']:
        SubflowResults[0] = 3
    else:
        SubflowResults[0] = 0
 
