flow BackToOpus():
    EventFlowSystemActor.UIMyDesignExchangeInfoHandlingEventFlowSystemActor.UIMyDesignExchangeInfoHandling('cSaveFromNet')
    if EventFlowSystemActor.MyDesignExchangeNetworkErrorEventFlowSystemActor.MyDesignExchangeNetworkError():
        run Obj_WorldMyDesignGallery_01_MainScene::MainUIrun Obj_WorldMyDesignGallery_01_MainScene::MainUI()
    else:
        switch EventFlowSystemActor.UIMyDesignDownloadWindowChoiceResultEventFlowSystemActor.UIMyDesignDownloadWindowChoiceResult():
            case 0:
                run BackToList()
            case 1:
                # The Custom Designs app will now open. Please select a save slot.
                EventFlowSystemActor.OpenMessageWindowEventFlowSystemActor.OpenMessageWindow('Dialog/DIALOG_Msg:2439', true)
                if EventFlowSystemActor.IsMyDesignProInMyDesignExchangeEventFlowSystemActor.IsMyDesignProInMyDesignExchange():
                    EventFlowSystemActor.UIMyDesignSelectHandlingEventFlowSystemActor.UIMyDesignSelectHandling(1, 2, 0, 0, 0)
                    switch EventFlowSystemActor.UIResultMenuDecideEventFlowSystemActor.UIResultMenuDecide():
                        default:
                            return
                else:
                    EventFlowSystemActor.UIMyDesignSelectHandlingEventFlowSystemActor.UIMyDesignSelectHandling(1, 1, 0, 0, 0)
                    switch EventFlowSystemActor.UIResultMenuDecideEventFlowSystemActor.UIResultMenuDecide():
                        default:
                            return
                run BackToOpus()
            case 2:
                EventFlowSystemActor.UINetConnectWaitAppearEventFlowSystemActor.UINetConnectWaitAppear()
                EventFlowSystemActor.NetDeleteMyDesignEventFlowSystemActor.NetDeleteMyDesign()
                EventFlowSystemActor.UINetConnectWaitDisappearEventFlowSystemActor.UINetConnectWaitDisappear()
                if EventFlowSystemActor.CheckErrorTypeEventFlowSystemActor.CheckErrorType(2):
                    System.EventFlags['cPlayerTemp:MyDesignExchangeCallMyIdList'] = false
                    run Obj_WorldMyDesignGallery_01_MainScene::MainUIrun Obj_WorldMyDesignGallery_01_MainScene::MainUI()
                else:
                    System.EventFlags['cPlayerTemp:MyDesignExchangeCallMyIdList'] = false
                    # The custom design has been deleted.
                    EventFlowSystemActor.OpenMessageWindowEventFlowSystemActor.OpenMessageWindow('Dialog/DIALOG_Msg:2435', false)
                    run Root()
 
